[$XDG_CONFIG_HOME/vcsh/repo.d/atom.git]
checkout =
    vcsh clone 'keybase://private/zxiiro/atom.git' 'atom'
    vcsh atom remote add github 'git@github.com:zxiiro/dotfiles-atom.git'
update   = vcsh atom pull
push     =
    vcsh atom push
    vcsh atom push github
status   = vcsh atom status
gc       = vcsh atom gc

